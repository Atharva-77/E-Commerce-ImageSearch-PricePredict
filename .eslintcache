    <Shipping />\n              </Route>\n\n              <Route path=\"/payment\">\n                 <Payment />\n              </Route>\n\n\n\n             {/* Admin routes */}\n              <Route path=\"/admin/userlist\">\n                 <Admin_UserList />\n              </Route>\n\n              <Route path=\"/admin/profile/:id\">\n                 <Admin_EditUser />\n              </Route>\n\n              <Route path=\"/admin/product\">\n                 <Admin_Product />\n              </Route> \n\n              <Route path=\"/admin/prod/edit/:id\">\n                 <Admin_EditProduct />\n              </Route>\n\n              <Route path=\"/admin/createProduct\">\n                 <Admin_CreateProduct />\n              </Route>\n\n              <Route path=\"/admin/orderlist\">\n                 <Admin_Order_List />\n              </Route>\n              {/* Admin Over */}\n\n\n\n\n              {/* Seller Routes */}\n              <Route path=\"/seller/product\">\n                 <ASeller_Product />\n              </Route> \n\n              <Route path=\"/seller/prod/edit/:id\">\n                 <ASeller_EditProduct />\n              </Route>\n\n              <Route path=\"/seller/createProduct\">\n                 <ASeller_CreateProduct />\n              </Route>\n\n              <Route path=\"/seller/orderlist\">\n                 <ASeller_OrderList />\n              </Route>\n               {/* Seller Over */}\n\n\n              <Route path=\"/placeorder/:id\">\n                 <OrderScreen />\n              </Route>\n\n              <Route path=\"/placeorder\">\n                 <PlaceOrder />\n              </Route>\n\n\n              <Route path=\"/checkout/:id?\"> \n              {/* or <Route path=\"/checkout/\">  */}\n                 <Header />        \n                 <Checkout />\n              </Route>\n\n\n              <Route path=\"/product/:id\"> \n                 <Header />        \n                 <ProductDetails />\n         